{
  "name": "admin",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "deploy": "remix vite:build && wrangler pages deploy",
    "dev": "remix vite:dev",
    "lint": "biome check --write ./app",
    "preview:local": "remix vite:build && vite preview",
    "start": "remix vite:build && wrangler pages dev",
    "type-check": "tsc",
    "typegen": "wrangler types",
    "biome": "biome",
    "migrate:local": "wrangler d1 execute edge-cms-auth-example --file './schema.sql' --local",
    "migrate:remote": "wrangler d1 execute edge-cms-auth-example --file './schema.sql' --remote",
    "reset:local": "rm -rf .wrangler/state/v3/d1 && pnpm migrate:local"
  },
  "dependencies": {
    "@cloudflare/kv-asset-handler": "^0.3.4",
    "@conform-to/react": "^1.2.2",
    "@conform-to/zod": "^1.2.2",
    "@radix-ui/react-avatar": "^1.1.0",
    "@radix-ui/react-collapsible": "^1.1.0",
    "@radix-ui/react-dialog": "^1.1.1",
    "@radix-ui/react-dropdown-menu": "^2.1.1",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-select": "^2.1.1",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-switch": "^1.1.0",
    "@radix-ui/react-tabs": "^1.1.0",
    "@radix-ui/react-toast": "^1.2.1",
    "@radix-ui/react-tooltip": "^1.1.2",
    "@remix-run/cloudflare": "^2.12.1",
    "@remix-run/react": "^2.12.1",
    "@repo/auth": "workspace:*",
    "@repo/core": "workspace:*",
    "@rjsf/core": "^5.21.1",
    "@rjsf/utils": "^5.21.1",
    "@rjsf/validator-ajv8": "^5.21.1",
    "@tabler/icons-react": "3.17.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "hono": "catalog:",
    "lucia": "^3.2.0",
    "lucide-react": "0.446.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwind-merge": "^2.5.2",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.23.8",
    "zustand": "4.5.5"
  },
  "devDependencies": {
    "@biomejs/biome": "catalog:",
    "@cloudflare/workers-types": "^4.20240512.0",
    "@hono/vite-dev-server": "^0.16.0",
    "@remix-run/dev": "^2.12.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.42",
    "tailwindcss": "^3.4.10",
    "typescript": "5.6.2",
    "vite": "catalog:",
    "vite-tsconfig-paths": "^5.0.1",
    "wrangler": "catalog:"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}